extends _layout
include templates/_global_mixins
include templates/_global_vars
include templates/_project_mixins
include templates/_project_vars
block vars
	- var array = {body_class:'', layuot_mod:'', sub_keywords:'',title_inner:'Title of the page'}
block content_block
	+form_element({
		type:'text',
		class_array:default_field,
		title:'some title',
		id:'id'
	})
	+form_element({
		type: 'select',
		class_array: default_field,
		title: 'some title',
		id: 'id',
		add_class: 'someclass',
		options: ['', 'Option 1', 'Option 2', 'Option 3']
	})
	+form_element({
		type: 'radio',
		class_array: default_field,
		title: 'some title',
		id: 'id',
		add_class: 'someclass',
		name:'radiobtn',
		checked:true
	})
	+form_element({
		type: 'checkbox',
		class_array: default_field,
		title: 'some title',
		id: 'id',
		add_class: 'someclass',
		name: 'radiobtn',
		checked: true
	})


